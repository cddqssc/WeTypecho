}
.cmicontext {
        margin-top: 5rpx;
        font-size: 24rpx;
        color: rgb(114, 114, 114);
        align-content: left;
}
.canvas-box{
        width: 100%;
        position: fixed;
        left: 0;
        top: 999999rpx;
}
.share-image {
        width: 600rpx;
        height: 888rpx;
        margin: 0 75rpx;
        border: 1px solid black;
}
.sharetext {
        margin-top: 36rpx;
        width: 100%;
        max-height: 100rpx;
        display: flex;
        justify-content: center;
        align-items: center;
