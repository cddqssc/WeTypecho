/* 基础样式 */

view,
scroll-view,
swiper,
swiper-item,
icon,
text,
progress,
button,
checkbox-group,
checkbox,
form,
input,
label,
picker,
radio-group,
radio,
slider,
switch,
action-sheet,
action-sheet-item,
action-sheet-cancel,
modal,
toast,
loading,
navigator,
audio,
image,
video,
map,
canvas {
        -webkit-box-sizing: border-box;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
}

page {
        height: 100%;
        font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

checkbox-group,
radio-group {
        display: block;
}

:before,
:after,
::before,
::after {
        -webkit-box-sizing: border-box;
        -moz-box-sizing: border-box;
        box-sizing: border-box;
}

.minheight {
        min-height: 60px;
}

.m_full {
        width: 100%;
        height: 100%;
}

.m-block {
        display: block;
}

.m_fullwidth {
        width: 100%;
}

.m_fullheight {
        height: 100%;
}

.detail_info {
        padding: 20rpx;
        padding-bottom: 5rpx;
        display: block;
        justify-content: flex-end;
        border-style: none none solid;
        border-width: 1px;
        border-color: rgb(196, 196, 196);
}

.detail_like_btn {
        border-radius: 50%;
        width: 70px;
        align-self: flex-end;
        order: 1;
        display: flex;
        height: 70px;
        background-color: rgb(255, 255, 255);
        flex-direction: column;
        align-items: center;
        justify-content: center;
        margin-top: 100;
        float: right;
        /* box-shadow: 0px 0px 3px 3px rgba(171, 171, 171, 0.8); */
        -ms-box-shadow: 0px 0px 3px 3px rgba(171, 171, 171, 0.8);
        -o-box-shadow: 0px 0px 3px 3px rgba(171, 171, 171, 0.8);
        -moz-box-shadow: 0px 0px 3px 3px rgba(171, 171, 171, 0.8);
        -webkit-box-shadow: 0px 0px 3px 3px rgba(171, 171, 171, 0.8);
}

.text_title {
        margin-right: 100px;
        font-size: 24px;
        font-family: "andale mono";
        font-weight: 600;
        margin-bottom: 10px;
        border-left-width: 8rpx;
        border-left-style: solid;
        border-left-color: #a70101;
        padding-left: 20rpx;
}

.view_section {
        padding-bottom: 2rpx;
        margin-left: 20rpx;
}

.view_section.section_actors {
        padding-right: 0px;
}

.commentlistitem {
        display: flex;
        margin-bottom: 25px;
}

.nav_userlink {
        align-self: flex-start;
}


.likeuser {
        width: 36px;
        height: 36px;
        background-image: url("http://imgcdn.guoku.com/avatar/128/782029b69f29e84d8f262c390bf3ab8c.jpg");
        background-position: 50% 50%;
        background-size: cover;
        border-radius: 48px;
        margin-bottom: 5px;
}

.view_comment {
        display: flex;
        justify-content: space-between;
        align-items: center;
        margin-bottom: 5px;
}

.comment_text {
        font-size: 13px;
        color: rgb(84, 84, 84);
        line-height: 1.5em;
        margin-bottom: 15px;
}

.comment_operate {
        display: flex;
        justify-content: space-between;
        align-items: center;
        margin-bottom: 5px;
}

.commentlikebtn {
        width: 14px;
        height: 14px;
}

.commentreplybtn {
        width: 16px;
        height: 16px;
}

.listitem_creattime {
        font-size: 12px;
        color: rgb(186, 186, 186);
        margin-right: 2px;
}

.text_section_title {
        font-weight: 600;
        margin-bottom: 20px;
        padding-left: 20rpx;
        border-left-width: 8rpx;
        border-left-style: solid;
        border-left-color: #a70101;
}

.index_swiper {
        height: 340rpx;
}

.index_swiper_post {

}

.detaillikeswp {
        height: 10px;
}
.detaillikeswpitem {
        height: 20px;
        width: 60px;
        background: #1AAD19;
}

.post_title {
        margin-top: 10px;
        left: 0px;
        right: 0px;
        top: 0px;
        bottom: 0px;
        align-self: flex-start;
        width: 100%;
        padding-left: 10px;
        padding-top: 10px;
        color: rgb(66, 66, 66);
}

.navigator_item {
        height: calc(100% - 30rpx);
        position: relative;
        display: flex;
        flex-direction: column;
        align-items: flex-end;
        background-color: rgb(122, 122, 122);
        background-position: 50% 0%;
        margin: 15rpx 0rpx 15rpx 20rpx;
        border-radius: 10rpx;
}

.index_header {
        justify-content: flex-start;
        min-height: 35px;
}

.text_more {
        font-size: 14px;
        color: #1AAD19;
}

.index_title {
        font-size: 16px;
        font-weight: 600;
}

.navigator_search {
        display: block;
        float: right;
}

.view_section.section_movies {
        padding-right: 0px;
}

.text_more.margin-right {
        margin-right: 20px;
}

.post_view {
        display: flex;
        padding-right: 10px;
        justify-content: flex-start;
        flex-flow: row nowrap;
}

.post_title_sub {
        font-size: 14px;
        font-weight: 600;
        margin-bottom: 5px;
}
.post_thumb {
        width: 120px;
        height: 72px;
}

.view_section.movielist {
        height: 100%;
}

.view_postlist_item {
        padding: 10px;
        border-style: solid;
        border-bottom-width: 1px;
        border-bottom-color: rgb(153, 153, 153);
        border-bottom-style: none;
        border-top-style: none;
        border-right-style: none;
        border-left-style: none;
}

.stat_list_item {
        padding: 8px;
        border-style: solid;
        border-bottom-width: 1px;
        border-bottom-color: rgb(196, 196, 196);
        border-top-style: none;
        border-right-style: none;
        border-left-style: none;
}

.image_cat_thumb {
        width: 180rpx;
        height: 130rpx;
        float: left;
        border-style: solid;
        border-radius: 10rpx;
        border-width: 2rpx;
        border-color: #d1d1d1;
        box-shadow: 0 0 6px rgba(30, 20, 20, 0.1);
}

.image_cat_box {
        margin-top: 15rpx;
        width: 66rpx;
        height: 100rpx;
        float: left;
}

.rank_cat_box {
        margin-top: 35rpx;
        width: 66rpx;
        height: 100rpx;
        float: left;
        font-size: 48rpx;
        text-align: center;
        color: #636262;
}

.view_sectionlist.movielist {
        height: 100%;
}

.view_catpost {
        width: 750rpx;
        margin-left: 1px;
        height: 100rpx;
        overflow: hidden;
}

.image_cat_item_title {
        display: -webkit-box;
        /* margin-left: 105px; */
        font-size: 30rpx;
        height: 40rpx;
        line-height: 40rpx;
        word-break: break-all;
        -webkit-box-orient: vertical;
        -webkit-line-clamp:2;
        font-weight: 400 !important;
        letter-spacing: 1px;
        overflow: hidden;
        text-overflow:ellipsis;
        padding-right: 60rpx;
}

.nav_postlist_item {
        border-style: none none solid;
        border-bottom-width: 0px;
        border-bottom-color: rgba(170, 170, 170, 1);
}
.nav_cat_item {
        height: auto;
        border-radius: 30rpx;
        font-size: 26rpx;
        color: #ffffff;
        font-weight: 500rpx;
        width: auto;
        padding: 5rpx 10rpx;
        line-height: 22px; /*垂直居中*/
        text-align: center;
}

.list_img_view {
        width: 24rpx;
        height: 24rpx;
        margin-left: 20rpx;
        vertical-align: middle;
}

.listpg_first_img {
        width: 24rpx;
        height: 24rpx;
        margin-left: 0rpx;
        vertical-align: middle;
}

.list_text_view {
        color: #959595;
        margin-left: 5rpx;
        font-size: 22rpx;
        line-height: 1.0;
        font-weight: normal;
        text-align: center;
}

.replay_view {
        background: #f7f7f7;
        padding: 10rpx;
        margin-right: 20rpx;
}

.replay_text {
        display: flex;
        align-items: center;
        font-size: 26rpx;
        font-weight: normal;
        outline: none;
        color: #000000;
        margin: 0rpx 0;
        margin-left: 20rpx;
}
.classify {
        display: flex;
        position: relative;
}

.classify .classify_catetory {
        width: 200rpx;
        position: relative;
        z-index: 1;
        border-right:  rgba(170, 170, 170, 1);
        border-right-style: solid;
        border-right-width: 2rpx;
}
.classify .classify_catetory .name {
        text-align: center;
        height: 100rpx;
        line-height: 100rpx;
        font-size: 28rpx;
        color: #666;
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
}
.scroll_box{
        width: 100%;
        overflow: hidden;
        padding: 30rpx 10rpx 20rpx;
        background: #fff;
        margin-left: 25rpx;
        }
.scroll_box .scroll-view_x {
        width: 100%;
        white-space: nowrap;
        overflow: hidden;
        border: 0 solid #ffffff;
        }

.item_list{
        width: auto;
        margin-right: 23rpx;
        display: inline-block;
        font-size: 30rpx;
        font-family: Verdana, Geneva, Tahoma, sans-serif;
        font-weight: 550;
}

.item_rank_list{
        width: 250rpx;
        margin-right: 0rpx;
        display: inline-block;
        font-size: 30rpx;
        font-family: Verdana, Geneva, Tahoma, sans-serif;
        font-weight: 550;
        text-align: center;
}

.active{
        font-size: 34rpx;
        padding-bottom: 12rpx;
        border-bottom: 6rpx solid rgb(167, 1, 1);
        background: #fff;
        color: rgb(167, 1, 1);
}
.classify .classify_catetory .active {
        border-left: 6rpx solid #ff6a3c;
        background: #fff;
        color: #ff6a3c;
}
.classify .classify_detail {
        position: relative;
        /* z-index: 999; */
        background: #fff;
        padding: 3rpx;
}
.classify .classify_detail .title {
        font-size: 30rpx;
        color: #333;
        margin-left: 25rpx;
}
.classify .classify_detail .children {
        display: flex;
        flex-wrap: wrap;
        margin: 0rpx 0rpx;
}
.classify .classify_detail .child_image {
        width: 60px;
        height: 60px;
}
.classify .classify_detail .child_name {
        font-size: 36rpx;
        color: #666;
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
        width: 400rpx;
}
.classify .classify_detail .children_item {
        height: 100rpx;
        width: 400rpx;
        text-align: left;
}

.footerview {
